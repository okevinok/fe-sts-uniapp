{
  "editor.formatOnSave": true,
  "volar.codeLens.pugTools": false,
  "eslint.format.enable": false,
  "eslint.runtime": "node",
  "files.autoSave": "off",
  "eslint.run": "onSave",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "amap",
    "Behaviour",
    "BEISEN",
    "Bipo",
    "Checkmark",
    "dagre",
    "Deps",
    "docxtemplater",
    "drakgray",
    "echarts",
    "Formily",
    "GULU",
    "hsts",
    "iconfont",
    "ismobilejs",
    "kubernetes",
    "lineheight",
    "mesoor",
    "MESOOR",
    "miniapp",
    "nadileaf",
    "NADILEAF",
    "pinia",
    "pizzip",
    "preload",
    "searchbar",
    "templater",
    "Tencent",
    "unauth",
    "unplugin",
    "usertype",
    "vite",
    "VITE",
    "vitejs",
    "vueuse",
    "wechat",
    "WECHAT",
    "weixin",
    "windi",
    "windicss",
    "workwechat",
    "WORKWECHAT"
  ],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.displayLanguage": "zh",
  "i18n-ally.enabledParsers": ["ts"],
  "i18n-ally.enabledFrameworks": ["vue-sfc", "vue"],
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.localesPaths": ["packages/**/**/locales"],
  "i18n-ally.pathMatcher": "{locale}.ts",
  "i18n-ally.keystyle": "nested",
  "vue.codeActions.enabled": false
}
